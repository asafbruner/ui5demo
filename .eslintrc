{
	"rules": {
		"block-scoped-var": 1,
		"brace-style": [0, "1tbs", {
			"allowSingleLine": true
		}],
		"consistent-this": 2,
		"global-strict": 1,
		"no-div-regex": 2,
		"no-floating-decimal": 2,
		"no-self-compare": 2,
		"no-mixed-spaces-and-tabs": [0, true],
		"no-nested-ternary": 2,
		"no-unused-vars": [0, {
			"vars": "all",
			"args": "none"
		}],
		"radix": 2,
		"space-after-keywords": [0, "always"],
		"space-unary-ops": 2,
		"wrap-iife": [0, "any"],
		"camelcase": 2,
		"consistent-return": 1,
		"max-nested-callbacks": [0, 3],
		"new-cap": 1,
		"no-extra-boolean-cast": 1,
		"no-lonely-if": 1,
		"no-new": 1,
		"no-new-wrappers": 1,
		"no-redeclare": 1,
		"no-unused-expressions": 1,
		"no-use-before-define": [0, "nofunc"],
		"no-warning-comments": 1,
		"strict": 1,
		"valid-jsdoc": [0, {
			"requireReturn": false
		}],
		"default-case": 1,
		"dot-notation": 0,
		"eol-last": 0,
		"eqeqeq": 0,
		"no-trailing-spaces": 0,
		"no-underscore-dangle": 0,
		"quotes": 0,
		"key-spacing": 0,
		"comma-spacing": 0,
		"no-multi-spaces": 0,
		"no-shadow": 0,
		"no-irregular-whitespace": 0
	},
	"globals": {
		"sap": true,
		"jQuery": true
	},
	"env": {
		"browser": true,
		"node": true
	},
	"customRulesPath": null
}